
public class Estudiante {

	//atributos
		int id;
		int edad;
		int telefono;
		int numeroDeNotas;
		double sumaDeNotas;
		double notaMedia;
		
		//constructor por defecto
		public Estudiante () {}
		
		//constructor parametrizado1
		public Estudiante (	int id, int edad, int telefono, int numeroDeNotas, double sumaDeNotas, double notaMedia) {
		this.id = id;
		this.edad = edad;
		this.telefono = telefono;
		this.numeroDeNotas = numeroDeNotas;
		this.sumaDeNotas = sumaDeNotas;
		this.notaMedia = notaMedia;
	}
		
		//constructor parametrizado2
		public Estudiante(int id, int edad) {
			this.id = id;
			this.edad = edad;
		
		
		//constructor parametrizado3
		public Estudiante(int id, int edad, int telefono) {
			this.id = id;
			this.edad = edad;
			this.telefono = telefono;
		}
			
			
		//métodos
		void mostrarInfo() {}
		void agregarNuevaNota() {}
		
}
