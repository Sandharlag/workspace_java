	public static void main(String[] args) {
		Scanner keyboard = new Scanner(System.in);
		System.out.print("Introduzca un nï¿½mero: ");
		int tabla = keyboard.nextInt();
		
		
		for(int i = 0 ; i < 10; i++) {
			System.out.printf(" i * tabla = %d", i * tabla).println();
		}
	
	
	}
	}